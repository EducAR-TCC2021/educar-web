{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "overrides": [
        {
            "files": ["**/*.test.js", "**/*.test.jsx"],
            "env": { "jest": true },
            "plugins": ["jest"]
        }
    ],
    "rules": {
        "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["state"] }],
        "linebreak-style": 0
    }
}
